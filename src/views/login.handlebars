<main id="loginMain container-sm">
  <section class="loginSection">
    <div class="loginDiv">
      <form id="loginForm">
        <div class="form-group">
          <label for="email">Correo Electrónico</label>
          <input type="email" name="email" class="form-control" id="email" placeholder="el_pibe_de_oro@gmail.com">
        </div>

        <div class="form-group">
          <label for="password">Contraseña</label>
          <input type="password" name="password" class="form-control" id="password" placeholder="Password">
        </div>

        <button type="submit" class="btn btn-primary">Iniciar Sesión</button>
      </form>
      <a href="/register" style="color: #ffff; margin-top: 20px;">¿No tenes cuenta? Create una pa.</a>


    </div>
  </section>
</main>
<script src="/js/login.js"></script>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>